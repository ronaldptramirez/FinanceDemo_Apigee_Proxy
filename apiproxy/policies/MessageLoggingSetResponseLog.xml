<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<MessageLogging async="false" continueOnError="false" enabled="true" name="MessageLoggingSetResponseLog">
    <DisplayName>MessageLogging.SetResponseLog</DisplayName>
    <File>
        <Message>API execution completed</Message>
        <FileName>FinanceDemoResponse.log</FileName>
